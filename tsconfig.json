{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "baseUrl": "./",
    "paths": {
      "@": [
        "src"
      ],
      "@screens": [
        "src/screens"
      ],
      "@utils": [
        "src/utils"
      ],
      "@redux": [
        "src/redux"
      ],
      "@images": [
        "assets/images"
      ],
      "@fonts": [
        "assets/fonts"
      ]
    }
  }
}
